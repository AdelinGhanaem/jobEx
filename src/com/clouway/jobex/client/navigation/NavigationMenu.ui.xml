<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:b='urn:import:com.github.gwtbootstrap.client.ui'>

  <ui:style>
    .nav {

      text-align: center;
      margin: 10px 0px 10px 0px;
    }


  </ui:style>

  <g:HTMLPanel addStyleNames="{style.nav}">
    <b:Navbar>
      <b:Brand text="JobEx"/>
      <b:Nav>
        <b:NavLink text="Home" ui:field="home"/>
        <b:NavLink text="Create new cv" ui:field="newCV"/>
        <b:NavLink text="Preview my cvs" ui:field="cvList"/>
        <b:NavLink text="Create new account" ui:field="newAccount"/>
      </b:Nav>
    </b:Navbar>
  </g:HTMLPanel>
</ui:UiBinder>