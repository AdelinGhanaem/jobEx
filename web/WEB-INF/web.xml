<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5">

  <servlet>
    <servlet-name>com.clouway.jobex.jobex ActionDispatcherService</servlet-name>
    <servlet-class>com.clouway.jobex.server.communication.ActionDispatcherServiceImpl</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>com.clouway.jobex.jobex ActionDispatcherService</servlet-name>
    <url-pattern>/jobex/ActionDispatcherService</url-pattern>
  </servlet-mapping>
</web-app>
